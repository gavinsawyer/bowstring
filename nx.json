{
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default"
    ],
    "sharedGlobals": []
  },
  "targetDefaults": {
    "eslint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore"
      ],
      "cache": true
    },
    "angular-browser": {
      "cache": true
    },
    "angular-server": {
      "cache": true
    },
    "angular-prerender": {
      "cache": true
    },
    "esbuild": {
      "cache": true
    },
    "tsc": {
      "cache": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "deploy": {
      "dependsOn": [
        "^deploy"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    }
  },
  "nxCloudAccessToken": "${NX_CLOUD_ACCESS_TOKEN}",
  "defaultBase": "main"
}
